{
  "name": "frisbee",
  "description": "Modern fetch-based alternative to axios/superagent/request. Great for React Native.",
  "version": "3.0.2",
  "author": "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
  "ava": {
    "failFast": true,
    "verbose": true,
    "files": [
      "test/**/*.test.js"
    ]
  },
  "bugs": {
